{
  "name": "Sunset Glow",
  "description": "温暖的日落主题，橘红色调营造温馨氛围",
  "version": "1.0.0",
  "appearance_mode": "auto",
  "colors": {
    "primary": {
      "light": "#FF6B35",
      "dark": "#FF8A65"
    },
    "secondary": {
      "light": "#FFB74D",
      "dark": "#FFD54F"
    },
    "accent": {
      "light": "#FF5722",
      "dark": "#FF7043"
    },
    "success": {
      "light": "#66BB6A",
      "dark": "#81C784"
    },
    "warning": {
      "light": "#FFCA28",
      "dark": "#FFD54F"
    },
    "error": {
      "light": "#EF5350",
      "dark": "#E57373"
    },
    "background": {
      "light": "#FFF8F5",
      "dark": "#1A0F0A"
    },
    "surface": {
      "light": "#FFEDE0",
      "dark": "#2D1B13"
    },
    "card": {
      "light": "#FFFFFF",
      "dark": "#3D251A"
    }
  },
  "typography": {
    "heading": {
      "font": "SF Pro Display",
      "size": 30,
      "weight": "bold"
    },
    "subheading": {
      "font": "SF Pro Display",
      "size": 18,
      "weight": "semibold"
    },
    "body": {
      "font": "SF Pro Text",
      "size": 16,
      "weight": "regular"
    },
    "caption": {
      "font": "SF Pro Text",
      "size": 12,
      "weight": "medium"
    }
  },
  "spacing": {
    "xs": 6,
    "sm": 12,
    "md": 18,
    "lg": 28,
    "xl": 36,
    "xxl": 52
  },
  "radius": {
    "sm": 6,
    "md": 10,
    "lg": 14,
    "xl": 18
  },
  "shadows": {
    "sm": {
      "offset": [0, 2],
      "blur": 4,
      "opacity": 0.12,
      "color": "#FF5722"
    },
    "md": {
      "offset": [0, 3],
      "blur": 8,
      "opacity": 0.16,
      "color": "#FF5722"
    },
    "lg": {
      "offset": [0, 5],
      "blur": 15,
      "opacity": 0.2,
      "color": "#FF5722"
    }
  },
  "animations": {
    "duration": {
      "fast": 0.2,
      "normal": 0.3,
      "slow": 0.5
    },
    "easing": {
      "ease_out": "cubic-bezier(0.23, 1, 0.32, 1)",
      "bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)"
    }
  },
  "effects": {
    "glass_intensity": 0.8,
    "blur_radius": 25,
    "vibrancy": "warm"
  },
  "metadata": {
    "author": "macUI Community",
    "created_at": "2024-08-26",
    "tags": ["sunset", "warm", "orange", "cozy"],
    "compatible_versions": ["2.0.0+"]
  }
}